package sort;

public class Sortieren {

	  public static void main (String [] args) {
		  int[] feld = {731, 243, 156, 438, 973, 123};;
		   int temp;
		   for (int i = 1; i < feld.length; i++) {
		    for (int j = i; j > 0; j--) {
		     if (feld[j] < feld [j - 1]) {
		      temp = feld[j];
		      feld[j] = feld[j - 1];
		      feld[j - 1] = temp;
		     }
		    }
		   }
		   for (int i = 0; i < feld.length; i++) {
		     System.out.println(feld[i]);
		   }
		  		    	   
		  }
	  	}
